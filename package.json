{
	"name": "youngjin",
	"scripts": {
		"dev": "firebase emulators:start",
		"devcss": "SKIP_CLIENT_COMPILE=true SKIP_SERVER_COMPILE=true SKIP_SSG=true npm run dev",
		"devclient": "SKIP_SERVER_COMPILE=true SKIP_SSG=true npm run dev",
		"devserver": "SKIP_CSS_COMPILE=true SKIP_CLIENT_COMPILE=true SKIP_SSG=true npm run dev",
		"devnossg": "SKIP_SSG=true npm run dev",
		"stop": "pm2 stop all && pm2 delete all",
		"beforeCommit": "npm run build && MODE=ssg node dist/server/bundle.js",
		"build": "npm run compileCSS && webpack --config build/webpack.config.server.js && webpack --config build/webpack.config.client.js",
		"compileCSS": "npx @tailwindcss/cli -i ./src/client/ui/style/input.css -o ./public/app/style_v2.css",
		"start": "node dist/server/bundle.js",
		"prepare": "husky"
	},
	"dependencies": {
		"@tailwindcss/cli": "^4.1.17",
		"@types/react": "^19.2.3",
		"@types/react-dom": "^19.2.2",
		"axios": "^1.13.2",
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.3",
		"cookie": "^1.0.2",
		"cookie-parser": "^1.4.7",
		"ejs": "^3.1.10",
		"express": "^4.21.2",
		"firebase-admin": "^13.6.0",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^9.0.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"tailwindcss": "^4.1.17",
		"three": "^0.180.0",
		"typescript-cookie": "^1.0.6"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cookie-parser": "^1.4.8",
		"@types/ejs": "^3.1.5",
		"@types/express": "^5.0.1",
		"@types/hpp": "^0.2.6",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/node": "^22.15.17",
		"@types/three": "^0.180.0",
		"husky": "^9.1.7",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.3",
		"webpack": "^5.99.8",
		"webpack-cli": "^6.0.1",
		"webpack-node-externals": "^3.0.0"
	}
}
