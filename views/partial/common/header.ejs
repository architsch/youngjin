<%_ const v_title = locals.title ? locals.title.replaceAll("\"", "&quot;").replaceAll("<", "&lt;").replaceAll(">", "&gt;") : "ThingsPool"; _%>
<%_ const v_desc = locals.desc ? locals.desc.replaceAll("\"", "&quot;") : "ThingsPool is an experimental web community for collaborative research and development in arts, games, and other forms of interactive media."; _%>
<%_ const v_keywords = locals.keywords ? locals.keywords.replaceAll("\"", "&quot;") : "thingspool, software toys, technical design, computer science, systems engineering, game design, game development"; _%>
<%_ const v_relativePageURL = locals.relativePageURL ? locals.relativePageURL : undefined; _%>
<%_ const v_ogImageURLOverride = locals.ogImageURLOverride ? locals.ogImageURLOverride : undefined; _%>
<%_ const v_pagePathList = locals.pagePathList ? locals.pagePathList : undefined; _%>
<%_ const v_backDestination_href = locals.backDestination_href ? locals.backDestination_href : undefined; _%>
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="description" content="<%= v_desc %>">
<meta name="keywords" content="<%= v_keywords %>">
<meta name="author" content="Youngjin Kang">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta property="og:title" content="<%= v_title %>"/>
<meta property="og:url" content="<%= EnvUtil.getRootURL() %>/<%= (v_relativePageURL != undefined) ? v_relativePageURL : '' %>"/>
<meta property="og:type" content="<%= (v_relativePageURL != undefined) ? 'article' : 'website' %>"/>
<meta property="og:site_name" content="ThingsPool"/>
<meta property="og:description" content="<%= v_desc %>"/>
<meta property="og:image" content="<%= (v_ogImageURLOverride != undefined) ? v_ogImageURLOverride : `${EnvUtil.getRootURL()}/share.jpg` %>"/>
<meta property="og:image:alt" content="<%= v_title %>">
<%_ if (!EnvUtil.isDevMode()) { _%>
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-JL7KHR7HK8"></script>
<script>
window.dataLayer = window.dataLayer || [];
function gtag(){dataLayer.push(arguments);}
gtag('js', new Date());
gtag('config', 'G-JL7KHR7HK8');
</script>
<%_ } _%>
<title><%= v_title %></title>
<link rel="shortcut icon" href="<%= EnvUtil.getRootURL() %>/favicon.ico">
<link rel="stylesheet" href="<%= EnvUtil.getRootURL() %>/style.css">
<link rel="author" href="https://www.linkedin.com/in/youngjin-kang-55321882">
<link rel="alternate" type="application/atom+xml" href="<%= EnvUtil.getRootURL() %>/feed.atom" title="Atom Feed">
<link rel="canonical" href="<%= (v_relativePageURL != undefined) ? `${EnvUtil.getRootURL()}/${v_relativePageURL}` : EnvUtil.getRootURL() %>">
</head>
<body>
<%_ if (!isStaticPage) { _%>
<%- include(`${ejsPartialRootPath}/common/coreUIDynamics.ejs`) %>
<%_ } _%>
<div class="fullscreenBar">
<div class="fullscreenBarLogo">
<img class="fullscreenBarLogoImage" src="<%= EnvUtil.getRootURL() %>/logo_light.png" alt="ThingsPool Logo">
</div>
<div class="fullscreenBarMenu">
<a class="fullscreenBarMenuButton <%= (menuName == "index") ? "selected" : "idle" %>" href="<%= EnvUtil.getRootURL() %>"><%= UIConfig.displayText.menuName["index"] %></a>
<a class="fullscreenBarMenuButton <%= (menuName == "rooms") ? "selected" : "idle" %>" href="<%= EnvUtil.getRootURL() %>/rooms.html"><%= UIConfig.displayText.menuName["rooms"] %></a>
<a class="fullscreenBarMenuButton <%= (menuName == "arcade") ? "selected" : "idle" %>" href="<%= EnvUtil.getRootURL() %>/arcade.html"><%= UIConfig.displayText.menuName["arcade"] %></a>
<a class="fullscreenBarMenuButton <%= (menuName == "library") ? "selected" : "idle" %>" href="<%= EnvUtil.getRootURL() %>/library.html"><%= UIConfig.displayText.menuName["library"] %></a>
</div>
</div>
<div class="fullscreenPanel">
<div class="s_row"></div>
<%_ if (v_backDestination_href != undefined) { _%>
<a class="inlineButton" href="<%= v_backDestination_href %>">Back</a><%- (v_pagePathList != undefined) ? "&nbsp;&nbsp;" : "" %>
<%_ } _%>
<%_ if (v_pagePathList != undefined) { _%>
<div class="pagePath"><%- v_pagePathList.map(x => (x.relativeURL != undefined) ? `<a href="${EnvUtil.getRootURL()}/${x.relativeURL}">${x.title}</a>` : x.title).join("/") %></div>
<%_ } _%>
<%_ if (v_pagePathList != undefined || v_backDestination_href != undefined) { _%>
<div class="s_row"></div>
<%_ } _%>