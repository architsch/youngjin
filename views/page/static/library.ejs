<%- include(`${ejsPartialRootPath}/common/header.ejs`, {
    url: `${process.env.URL_STATIC}/library.html`,
    pageName: "library",
    pagePathList: [
        {title: UIConfig.displayText.pageName["index"], url: process.env.URL_STATIC},
        {title: UIConfig.displayText.pageName["library"], url: undefined},
    ],
    backDestination_href: process.env.URL_STATIC,
}) %>
<%_ for (const [category, entries] of Object.entries(entriesByCategory)) { _%>
<hr>
<h1><%- category %></h1>
<%_ for (let i = entries.length-1; i >= 0; --i) { _%>
<%_ const entry = entries[i]; _%>
<a class="postEntryButton" href="<%= process.env.URL_STATIC %>/<%= entry.dirName %>/list.html"><%- entry.title %></a>
<%_ } _%>
<%_ } _%>
<hr>
<h1>Games</h1>
<%- include(`${ejsPartialRootPath}/info/gameLinks.ejs`, { entries: gameEntries }) %>
<%- include(`${ejsPartialRootPath}/common/footer.ejs`) %>