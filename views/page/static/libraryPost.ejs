<%- include(`${ejsPartialRootPath}/common/header.ejs`, {
    title: title,
    desc: desc,
    keywords: keywords,
    ogImageURLOverride: customOGImagePath,
    url: `${URL_STATIC}/${entry.dirName}/page-${pageNumber}.html`,
    ogType: "article",
    pageName: "library",
    pagePathList: [
        {title: PAGE_NAME_MAP["index"], url: URL_STATIC},
        {title: PAGE_NAME_MAP["library"], url: `${URL_STATIC}/library.html`},
        {title: entry.title, url: `${URL_STATIC}/${entry.dirName}/list.html`},
        {title: title, url: undefined},
    ],
    backDestination_href: `${URL_STATIC}/${entry.dirName}/list.html`,
}) %>
<%- content _%>
<div class="s_row"></div>
<%_ if (pageNumber > 1) { _%>
    <a class="inlineButton" href="<%= URL_STATIC %>/<%= entry.dirName %>/page-<%= pageNumber-1 %>.html">Previous Page</a>
<%_ } _%>
<%_ if (!isLastPage) { _%>
    <a class="inlineButton" href="<%= URL_STATIC %>/<%= entry.dirName %>/page-<%= pageNumber+1 %>.html">Next Page</a>
<%_ } _%>
<%- include(`${ejsPartialRootPath}/common/footer.ejs`) %>